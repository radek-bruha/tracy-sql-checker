{
    "name": "radek-bruha/tracy-sql-checker",
    "description": "Tracy Sql Checker Extension",
    "license": "unlicense",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Bruha\\Tracy\\": "src"
        }
    },
    "require": {
        "php": "^8.2",
        "doctrine/orm": "^2.16",
        "tracy/tracy": "^2.10"
    },
    "require-dev": {
        "radek-bruha/coding-standard": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": false,
            "cweagans/composer-patches": false,
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
